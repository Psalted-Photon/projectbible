{
  "name": "projectbible",
  "private": true,
  "version": "0.0.0",
  "packageManager": "npm@10.2.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev:pwa": "npm -w apps/pwa run dev",
    "build:pwa": "npm -w apps/pwa run build",
    "lint": "npm -ws run lint",
    "typecheck": "npm -ws run typecheck",
    "test": "npm -ws run test",
    "packtools": "npm -w packages/packtools run cli"
  }
}
